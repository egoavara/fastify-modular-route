{
  "name": "fastify-modular-route",
  "version": "0.6.0",
  "description": "route template for fastify-modular ",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    }
  },
  "files": [
    "cjs/**/*.d.ts",
    "cjs/**/*.js",
    "cjs/package.json",
    "esm/**/*.d.ts",
    "esm/**/*.js",
    "esm/package.json"
  ],
  "scripts": {
    "build": "npm run build:cjs & npm run build:esm",
    "build:cjs": "rimraf \"cjs/**/*.{js,js.map,d.ts,d.ts.map}\" && tsc -p ./cjs/tsconfig.json",
    "build:esm": "rimraf \"esm/**/*.{js,js.map,d.ts,d.ts.map}\" && tsc -p ./esm/tsconfig.json",
    "watch:cjs": "rimraf \"cjs/**/*.{js,js.map,d.ts,d.ts.map}\" && tsc --watch -p ./cjs/tsconfig.json",
    "watch:esm": "rimraf \"esm/**/*.{js,js.map,d.ts,d.ts.map}\" && tsc --watch -p ./esm/tsconfig.json",
    "test": "tap --ts",
    "postversion": "npm publish",
    "prepack": "npm run build",
    "prepublish": "cross-var \"git tag -a \\\"v$npm_package_version\\\" -m \\\"version $npm_package_version\\\"\""
  },
  "keywords": [
    "typescript",
    "fastify",
    "fastify-modular"
  ],
  "author": "egoavara",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.33",
    "@types/tap": "^15.0.6",
    "cross-var": "^1.1.0",
    "rimraf": "^3.0.2",
    "tap": "^16.0.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "pito": "^0.1.3"
  }
}
