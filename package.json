{
  "name": "fastify-modular-route",
  "version": "0.0.7",
  "description": "route template for fastify-modular ",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    }
  },
  "files": [
    "cjs/**/*.d.ts",
    "cjs/**/*.d.ts.map",
    "cjs/**/*.js",
    "cjs/**/*.js.map",
    "cjs/package.json",
    "esm/**/*.d.ts",
    "esm/**/*.d.ts.map",
    "esm/**/*.js",
    "esm/**/*.js.map",
    "esm/package.json"
  ],
  "scripts": {
    "build": "npm run \"build:cjs\" & npm run \"build:esm\"",
    "build:cjs": "rimraf \"cjs/**/*.{js,js.map,d.ts,d.ts.map}\" && tsc -p ./cjs/tsconfig.json",
    "build:esm": "rimraf \"esm/**/*.{js,js.map,d.ts,d.ts.map}\" && tsc -p ./esm/tsconfig.json",
    "watch": "npm run \"watch:cjs\" & npm run \"watch:esm\"",
    "watch:cjs": "rimraf \"cjs/**/*.{js,js.map,d.ts,d.ts.map}\" && tsc --watch -p ./cjs/tsconfig.json",
    "watch:esm": "rimraf \"esm/**/*.{js,js.map,d.ts,d.ts.map}\" && tsc --watch -p ./esm/tsconfig.json",
    "test": "tap --ts"
  },
  "keywords": [
    "typescript",
    "fastify",
    "fastify-modular"
  ],
  "author": "egoavara",
  "license": "MIT",
  "devDependencies": {
    "@types/tap": "^15.0.6",
    "rimraf": "^3.0.2",
    "tap": "^16.0.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "pito": "^0.0.4"
  }
}