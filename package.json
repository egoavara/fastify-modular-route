{
  "name": "@fastify-modular/route",
  "version": "1.1.0",
  "description": "route template for fastify-modular ",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "types": "./types/index.d.ts",
      "require": "./cjs/index.js"
    }
  },
  "files": [
    "cjs/**/*.js",
    "cjs/package.json",
    "esm/**/*.js",
    "esm/package.json",
    "types/**/*.d.ts"
  ],
  "scripts": {
    "build": "rimraf \"{cjs,esm,types}/**/*.{js,js.map,d.ts,d.ts.map}\" && tsc -b ./cjs/tsconfig.json ./esm/tsconfig.json ./types/tsconfig.json",
    "watch": "rimraf \"{cjs,esm,types}/**/*.{js,js.map,d.ts,d.ts.map}\" && tsc -b ./cjs/tsconfig.json ./esm/tsconfig.json ./types/tsconfig.json --watch",
    "test": "tap --ts",
    "postversion": "npm publish --access=public",
    "prepack": "npm run build"
  },
  "keywords": [
    "typescript",
    "fastify",
    "fastify-modular"
  ],
  "author": "egoavara",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.33",
    "@types/tap": "^15.0.6",
    "cross-var": "^1.1.0",
    "rimraf": "^3.0.2",
    "tap": "^16.0.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "pito": "~1.0.0"
  },
  "peerDependencies": {
    "pito": "~1.0.0"
  }
}
